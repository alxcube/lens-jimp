{
  "name": "@alxcube/lens-jimp",
  "version": "3.0.0",
  "description": "Jimp adapter for @alxcube/lens library.",
  "type": "module",
  "main": "./dist/lens-jimp.umd.cjs",
  "module": "./dist/lens-jimp.js",
  "types": "./dist/lens-jimp.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lens-jimp.js",
      "require": "./dist/lens-jimp.umd.cjs"
    }
  },
  "scripts": {
    "build": "npm run lint && npm run typecheck && vite build",
    "typecheck": "tsc",
    "test": "jest",
    "lint": "eslint ./src --ext .ts && npm run prettier",
    "prettier": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "lens",
    "jimp",
    "lens adapter",
    "lens node"
  ],
  "author": "Alexander Alexandrov <alxcube@gmail.com>",
  "license": "Apache-2.0",
  "files": [
    "dist/",
    "src/"
  ],
  "dependencies": {
    "@alxcube/lens": "^2.0.0",
    "jimp": "^0.22.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "vite": "^5.0.6",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-dts": "^3.6.4"
  }
}
